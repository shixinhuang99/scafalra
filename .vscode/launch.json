{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "scafalra-debug",
      "skipFiles": ["<node_internals>/**", "${workspaceFolder}/node_modules/**/*.js"],
      "program": "${workspaceFolder}/src/cli.ts",
      "preLaunchTask": "npm: debug",
      "outFiles": ["${workspaceFolder}/debug/**/*.js"],
      "env": { "NODE_ENV": "dev" },
      "args": []
    }
  ]
}

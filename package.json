{
  "name": "@zerowong/scaffold-cli",
  "version": "0.1.2",
  "description": "scaffold commannd line tool",
  "author": "zerowong <wongzero@foxmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/zerowong/scaffold-cli",
  "bugs": "https://github.com/zerowong/scaffold-cli/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/zerowong/scaffold-cli.git"
  },
  "type": "commonjs",
  "bin": "bin/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "scaffold",
    "cli",
    "template"
  ],
  "scripts": {
    "start": "NODE_ENV=test esno ./src/bin.ts",
    "build": "tsup",
    "test": "vitest --run",
    "test:w": "vitest",
    "test:b": "npm run build && vitest --run",
    "test:c": "vitest --run --coverage"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "mri": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "c8": "^7.11.2",
    "eslint": "^8.13.0",
    "esno": "^0.14.1",
    "prettier": "^2.6.2",
    "tsup": "^5.12.5",
    "typescript": "^4.6.3",
    "vitest": "^0.9.3"
  }
}

{
  "name": "scafalra",
  "version": "0.5.0",
  "description": "command-line interface tool for manage scaffolding",
  "author": "shixinhuang99 <shixinhuang99@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/shixinhuang99/scafalra#readme",
  "bugs": "https://github.com/shixinhuang99/scafalra/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/shixinhuang99/scafalra.git"
  },
  "type": "module",
  "bin": {
    "scafalra": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "keywords": [
    "scafalra",
    "scaffold",
    "template",
    "boilerplate",
    "command-line"
  ],
  "scripts": {
    "dev": "NODE_ENV=dev tsx ./src/cli.ts",
    "build": "tsc --project ./tsconfig.build.json",
    "test": "vitest",
    "test:cov": "vitest --coverage",
    "postversion": "git push && git push --tags",
    "clean": "node ./scripts/clean.js",
    "typecheck": "tsc --noEmit",
    "debug": "tsc --project ./tsconfig.debug.json"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "chalk": "^5.0.1",
    "https-proxy-agent": "^5.0.1",
    "mri": "^1.2.0",
    "node-stream-zip": "^1.15.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitest/coverage-c8": "^0.24.3",
    "c8": "^7.12.0",
    "eslint": "^8.21.0",
    "prettier": "^2.7.1",
    "tsx": "^3.9.0",
    "typescript": "^4.8.2",
    "vitest": "^0.24.3"
  },
  "packageManager": "pnpm@7.9.5"
}
